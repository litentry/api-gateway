{
  "name": "substrate-api-proxy",
  "version": "0.0.1",
  "description": "GraphQL API as proxy for substrate networks",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "repository": "git@github.com:litentry/web3-indexer.git",
  "contributors": [
    "Ben Mason <ben@wattmason.co.uk>",
    "Francisco Silva <franjs.francisco@gmail.com>"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "node 'dist/index.js",
    "build:dev": "nodemon 'src/index.ts' --exec 'ts-node' src/index.ts -e ts,graphql",
    "start:dev": "yarn build:dev"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.3.1",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^16.0.1",
    "graphql-ws": "^5.5.5",
    "ws": "^8.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "eslint": "7.26.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.3.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
